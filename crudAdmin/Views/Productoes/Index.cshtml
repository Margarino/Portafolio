@model IEnumerable<crudAdmin.Models.Producto>

@{
    ViewData["Title"] = "Index";
}

<h1>Índice</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                <a>Descripción del producto</a>
            </th>
            <th>
                <a>Valor del producto</a>
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Descripcionproducto)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Valorproducto)
            </td>
            <td>
                <a asp-action="Details" asp-route-id="@item.Idproducto">Details</a>
            </td>
        </tr>
}
    </tbody>
</table>
