@model IEnumerable<crudAdmin.Models.Mesa>

@{
    ViewData["Title"] = "Principal";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Aforomaximo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Estadomesa)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Aforomaximo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Estadomesa)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Idmesa">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Idmesa">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Idmesa">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
