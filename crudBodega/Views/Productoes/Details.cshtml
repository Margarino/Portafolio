@model crudBodega.Models.Producto

@{
    ViewData["Title"] = "Details";
}

<h1>Detalles</h1>

<div>
    <h4>Producto</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            <a>Descripción Producto</a>
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Descripcionproducto)
        </dd>
        <dt class = "col-sm-2">
            <a>Valor Producto</a>
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Valorproducto)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.Idproducto">Editar</a> |
    <a asp-action="Index">Volver a la lista</a>
</div>
